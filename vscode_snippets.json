{
    "Sequence": {
        "prefix": "seq",
        "body": "($1_${2:n})_{${2:n}\\\\in ${3:\\\\N}}$0",
        "description": "full sequence notation"
    },
    "Partition": {
        "prefix": "part",
        "body": "(${1:x}_${2:i})_{${2:i}= ${3:0}}^${4:n}$0",
        "description": "full sequence notation"
    },
    "Parital Sequence": {
        "prefix": "pseq",
        "body": "($1_{${2:n}_{${3:k}}})_{${3:k}\\\\in ${4:\\\\N}}$0",
        "description": "partial sequence notation"
    },
    "Series": {
        "prefix": "ser",
        "body": "\\\\sum_{${1:k}=${2:1}}^{${3:\\\\infty}}$4_{${1:k}}",
        "description": "full series notation"
    },
    "Power Series": {
        "prefix": "pow",
        "body": "\\\\sum_{${1:k}=${2:0}}^${3:\\\\infty}$0",
        "description": "Power Series"
    },
    "Set of Limit Point": {
        "prefix": "hset",
        "body": "\\\\mathcal{H}(($1_{${2:n}})_{${2:n}\\\\in ${3:\\\\N}})$0",
        "description": "set of all limit-points"
    },
    "Open Ball": {
        "prefix": "opb",
        "body": "\\\\mathcal{B}_{${1:\\\\varepsilon}}(${2:x_0})$0",
        "description": "open ball"
    },
    "Converges Against": {
        "prefix": "conv",
        "body": "\\\\overset{${1:n}\\\\rightarrow ${2:\\\\infty}}{\\\\longrightarrow}$0",
        "description": "short limit notation"
    },
    "Conjugate": {
        "prefix": "conj",
        "body": "\\\\overline{$1}",
        "description": "complex conjugate"
    },
    "Fraction": {
        "prefix": "frac",
        "body": "\\\\frac{$1}{$2}",
        "description": "fraction"
    },
    "Sum": {
        "prefix": "sum",
        "body": "\\\\sum_{$1${4:=} $2}^{$3}",
        "description": "sum"
    },
    "Integral": {
        "prefix": "int",
        "body": "${1:\\\\int}_{${2:}}^{${3:}}${4:f}(${5:x})\\idd ${5:x}",
        "description": "basic integral"
    },
    "Definition": {
        "prefix": "def",
        "body": "\\\\begin{definition}{$1}{$2}\n\t$0\n\\\\end{definition}",
        "description": "create definition environment"
    },
    "Theorem": {
        "prefix": "the",
        "body": "\\\\begin{theorem}{$1}{$2}\n\t$0\n\\\\end{theorem}",
        "description": "create theorem environment"
    },
    "task": {
        "prefix": "task",
        "body": "\\\\begin{task}{$1}{$2}\n\t$0\n\\\\end{task}",
        "description": "create task environment"
    },
    "Lemma": {
        "prefix": "lem",
        "body": "\\\\begin{lemma}{$1}{$2}\n\t$0\n\\\\end{lemma}",
        "description": "create lemma environment"
    },
    "Remark": {
        "prefix": "rem",
        "body": "\\\\begin{remark}{$1}{$2}\n\t$0\n\\\\end{remark}",
        "description": "create remark environment"
    },
    "Corollary": {
        "prefix": "cor",
        "body": "\\\\begin{corollary}{$1}{$2}\n\t$0\n\\\\end{corollary}",
        "description": "create corollary environment"
    },
    "Enumerate": {
        "prefix": "enum",
        "body": "\\\\begin{enumerate}\n\t\\\\item $0\n\\\\end{enumerate}",
        "description": "create enumeration environment"
    },
    "Roman Enumerate": {
        "prefix": "rnum",
        "body": "\\\\begin{enumerate}[label=\\\\roman*]\n\t\\\\item $0\n\\\\end{enumerate}",
        "description": "create enumeration environment with roman labelling"
    },
    "Alph Enumerate": {
        "prefix": "anum",
        "body": "\\\\begin{enumerate}[label=\\\\alph*]\n\t\\\\item $0\n\\\\end{enumerate}",
        "description": "create enumeration environment with alphabetic numbering"
    },
    "Limit": {
        "prefix": "lim",
        "body": "\\\\lim_{${1:n}\\\\subj ${2:\\\\infty}}$0",
        "description": "create limit"
    },
    "Differential Quotient": {
        "prefix": "diffq",
        "body": "\\\\lim_{${1:x}\\\\subj ${2:x_0}}\\\\frac{${3:f}(${1:x})-${3:f}(${2:x_0})}{${1:x}-${2:x_0}}$0",
        "description": "insert differential quotient"
    },
    "Differential Sequence Quotient": {
        "prefix": "siffq",
        "body": "\\\\lim_{${1:n}\\\\subj ${2:\\infty}}\\\\frac{${3:f}(${4:\\xi_n})-${3:f}(${5:x_0})}{${4:\\xi_n}-${5:x_0}}$0",
        "description": "insert differential quotient"
    },
    "Derivative": {
        "prefix": "der",
        "body": "\\\\frac{\\\\dd ${1:f}}{\\\\dd ${2:x}}$0",
        "description": "first derivative"
    },
    "nth-Derivative": {
        "prefix": "nder",
        "body": "\\\\frac{\\\\dd^{${3:n}} ${1:f}}{\\\\dd ${2:x}^{${3:n}}}$0",
        "description": "nth derivative"
    },
    "Partial Derivative": {
        "prefix": "per",
        "body": "\\\\frac{\\\\partial ${1:f}}{\\\\partial ${2:x}}$0",
        "description": "first partial derivative"
    },
    "nth Partial Derivative": {
        "prefix": "nper",
        "body": "\\\\frac{\\\\partial^{${3:n}} ${1:f}}{\\\\dd ${2:x}^{${3:n}}}$0",
        "description": "nth partial derivative"
    },
    "Landau O": {
        "prefix": "landau",
        "body": "\\\\mathcal{O}($1)$0"
    },
    "Secant": {
        "prefix": "sec",
        "body": "\\\\frac{${1:f}(${2:b}) - ${1:f}(${3:a})}{${2:b} - ${3:a}}"
    },
    "abs": {
        "prefix": "abs",
        "body": "|$1|$0"
    },
    "norm": {
        "prefix": "norm",
        "body": "\\\\|${1:\\\\cdot}\\\\|"
    },
    "chi": {
        "prefix": "chi",
        "body": "\\\\chi_{[${1:a},${2:b}]}$0"
    },
    "diffq+": {
        "prefix": "diffq+",
        "body": "\\\\lim_{${1:x}\\\\subj ${2:x_0}^+}\\\\frac{${3:f}(${1:x})-${3:f}(${2:x_0})}{${1:x}-${2:x_0}}$0",
        "description": "insert right-sided differential quotient"
    },
    "diffq-": {
        "prefix": "diffq-",
        "body": "\\\\lim_{${1:x}\\\\subj ${2:x_0}^-}\\\\frac{${3:f}(${1:x})-${3:f}(${2:x_0})}{${1:x}-${2:x_0}}$0",
        "description": "insert left-sided differential quotient"
    },
    "bound": {
        "prefix": "fbnd",
        "body": "${1:f}\\big\\vert_{$2}$0"
    },
    "closed interval":{
        "prefix": "inter",
        "body": "[$1,$2]$0"
    },
    "lclosed interval":{
        "prefix": "lint",
        "body": "[$1,$2)$0"
    },
    "rclosed interval":{
        "prefix": "rint",
        "body": "($1,$2]$0"
    },
    "open interval":{
        "prefix": "ont",
        "body": "($1,$2)$0"
    },
    "stem eval": {
        "prefix": "stem",
        "body": "${1:F}\\Big\\vert_{$2}^{$3}$0"
    }, 
    "limsup": {
        "prefix": "limsup",
        "body": "\\limsup_{${1:n}\\subj ${2:\\infty}}$0"
    },
    "Vector sequence": {
        "prefix": "vseq",
        "body": "(\\vec{${1:v}}^${2:k})_{${2:k}\\in ${3:\\N}}$0"
    },
    "Component Sequence": {
        "prefix": "cseq",
        "body": "(${1:x}_${2:i}^${3:k})_{${3:k}\\in ${4:\\N}}$0"
    },
    "Partial Component Sequence": {
        "prefix": "pcseq",
        "body": "(${1:x}_${2:i}^{${3:k}_{${4:j}_${2:i}}})_{${4:j}_${2:i}\\in ${5:\\N}}$0"
    }
}